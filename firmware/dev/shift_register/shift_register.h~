#ifndef SHIFT_H
#define SHIFT_H

#include "sys/port.h"
#include "lib/delay.h"

typedef struct Shift_register{
	byte data;
	Pin *serial;
	Pin *latch;
	Pin *clk;
}Shift_register;

Shift_register shift_register_new(byte data, Pin *serial, Pin *clk, Pin *latch);
void shift_register_latch_output(Shift_register *shift_register);


#endif
